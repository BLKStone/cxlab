<h1>Listing manage_configs</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
      <th>Field type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manage_configs.each do |manage_config| %>
      <tr>
        <td><%= manage_config.key %></td>
        <td><%= manage_config.value %></td>
        <td><%= manage_config.field_type %></td>
        <td><%= link_to 'Show', manage_config %></td>
        <td><%= link_to 'Edit', edit_manage_config_path(manage_config) %></td>
        <td><%= link_to 'Destroy', manage_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Config', new_manage_config_path %>
