<h1>Listing manage_nodes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Remark</th>
      <th>Extra data</th>
      <th>Sort</th>
      <th>Pid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manage_nodes.each do |manage_node| %>
      <tr>
        <td><%= manage_node.name %></td>
        <td><%= manage_node.title %></td>
        <td><%= manage_node.remark %></td>
        <td><%= manage_node.extra_data %></td>
        <td><%= manage_node.sort %></td>
        <td><%= manage_node.pid %></td>
        <td><%= link_to 'Show', manage_node %></td>
        <td><%= link_to 'Edit', edit_manage_node_path(manage_node) %></td>
        <td><%= link_to 'Destroy', manage_node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Node', new_manage_node_path %>
